<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.temp_03_03">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.mainActivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.sensorListActivity.SensorListActivity"
            android:parentActivityName=".activities.mainActivity.MainActivity"/>
        <activity android:name=".activities.accelerometerActivity.AccelerometerDataActivity"
            android:parentActivityName=".activities.mainActivity.MainActivity"/>
        <activity android:name=".activities.gpsDataActivity.GpsDataActivity"
            android:parentActivityName=".activities.mainActivity.MainActivity"/>
    </application>

</manifest>